  "dependencies": {
    "@elysiajs/static": "^0.8.0",
    "@elysiajs/swagger": "^0.8.0",
    "@neondatabase/serverless": "^0.8.1",
    "drizzle-orm": "^0.29.3",
    "elysia": "^0.8.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "bun-types": "^1.0.26",
    "drizzle-kit": "^0.20.14"
  },
  "module": "src/server.ts"
}
